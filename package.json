{
  "name": "toy-a",
  "version": "0.3.0",
  "type": "module",
  "description": "A toy project to experiment with npm packaging",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "cli": "tsx src/cli.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build": "rimraf ./dist && npm run build:types && rollup -c",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microu-mr-kit/toy-a.git"
  },
  "keywords": [
    "npm",
    "typescript",
    "rollup"
  ],
  "author": "Microu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microu-mr-kit/toy-a/issues"
  },
  "homepage": "https://github.com/microu-mr-kit/toy-a#readme",
  "dependencies": {
    "lorem-ipsum": "^2.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.12",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.1",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
